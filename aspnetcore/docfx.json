{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md"
        ],
        "exclude": [
          "**/obj/**",
          "aspnet/**",
          "**/includes/**",
          "**/aspnet-core-conceptual/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "aspnet/core/**",
          "**/includes/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {"breadcrumb_path": "/aspnet/breadcrumb/toc.json"},
    "fileMetadata": {},
    "template": [],
    "dest": "_site"
  }
}