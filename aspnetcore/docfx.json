{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md"
        ],
        "exclude": [
          "**/obj/**",
          "**/sample/**",
          "**/samples/**",
          "aspnet/**",
          "**/includes/**",
          "**/aspnet-core-conceptual/**",
          "_site/**"
        ],
        "group": "group1",
        "src": "."
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "aspnet/core/**",
          "**/includes/**",
          "_site/**"
        ],
        "group": "group1",
        "src": "."
      }
    ],
    "overwrite": [],
    "externalReference": [],    
    "globalMetadata": {
      "_navPath": "/foo",
      "_navRel": "/foo",
      "breadcrumb_path": "/aspnet/breadcrumb/toc.json",
      "feedback_github_repo": "aspnet/docs",
      "feedback_product_url": "https://github.com/aspnet/Home/blob/dev/CONTRIBUTING.md",
      "feedback_system": "GitHub",
      "manager": "wpickett",
      "ms.prod": "aspnet-core",
      "ms.topic": "conceptual",
      "searchScope": [
        "ASP.NET Core"
      ]
    },
    "fileMetadata": {
      "ms.technology": {
        "razor-pages/**/**.md": "aspnetcore-razorpages",
        "signalr/**/**.md": "aspnetcore-signalr",
        "web-api/**/**.md": "aspnetcore-webapi"
      }
    },
    "template": [],
    "dest": "_site",
    "markdownEngineName": "markdig",
    "groups": {
      "group1": {
        "dest": "group1-dest",
        "moniker_range": ">= aspnetcore-1.0"
      }
    }
  }
}
